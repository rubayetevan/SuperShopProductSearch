<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.warriors.groups.supershopproductsearch">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProductSearchActivity"
            android:label="@string/title_activity_product_search" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".ProductSettingsActivity"
            android:label="@string/title_activity_product_settings" />
        <activity
            android:name=".ProductAddActivity"
            android:label="@string/title_activity_product_add" />
        <activity
            android:name=".ProductDeleteActivity"
            android:label="@string/title_activity_product_delete" />
        <activity
            android:name=".ProductUpdateActivity"
            android:label="@string/title_activity_product_update" />
        <activity
            android:name=".ProductDetailsActivity"
            android:label="Product Details"/>
        <activity
            android:name=".UpdateUserActivity"
            android:label="Update User" />
    </application>

</manifest>
